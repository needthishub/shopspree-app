rchase));\r\n    };\r\n\r\n    const renderBestSellerProducts = () => {\r\n        return bestSellerProducts.map((product) => {\r\n            return (\r\n                <ProductCard addToCart={handleAddToCart} product={product} key={product.id}/>\r\n            );\r\n        });\r\n    };\r\n\r\n    return (\r\n        <div className=\"best-seller-container\">\r\n            <h2>Best Seller</h2>\r\n            <div className=\"best-seller-products\">\r\n                {renderBestSellerProducts()}\r\n            </div>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default BestSeller;\r\n","C:\\Users\\Ioan\\WebstormProjects\\ecom-shop-react-ts-21\\src\\index.tsx",[],"C:\\Users\\Ioan\\WebstormProjects\\ecom-shop-react-ts-21\\src\\constants\\route.ts",[],"C:\\Users\\Ioan\\WebstormProjects\\ecom-shop-react-ts-21\\src\\store\\rootReducer.ts",[],"C:\\Users\\Ioan\\WebstormProjects\\ecom-shop-react-ts-21\\src\\reportWebVitals.ts",[],"C:\\Users\\Ioan\\WebstormProjects\\ecom-shop-react-ts-21\\src\\components\\Partners\\index.tsx",[],"C:\\Users\\Ioan\\WebstormProjects\\ecom-shop-react-ts-21\\src\\components\\CustomerInformation\\utils.ts",[],"C:\\Users\\Ioan\\WebstormProjects\\ecom-shop-react-ts-21\\src\\customHooks\\useLayoutEffectOnUpdate.ts",["229","230"],"import {useLayoutEffect, useRef} from \"react\";\r\n\r\nexport const useLayoutEffectOnUpdate = (effect: React.EffectCallback, deps?: React.DependencyList | undefined) => {\r\n    const initialRender = useRef(true);\r\n\r\n    useLayoutEffect(() => {\r\n        if (initialRender.current) {\r\n            initialRender.current = false;\r\n        } else {\r\n            effect();\r\n        }\r\n    }, deps);\r\n\r\n}\r\n","C:\\Users\\Ioan\\WebstormProjects\\ecom-shop-react-ts-21\\src\\store\\reducers\\userReducer.ts",[],"C:\\Users\\Ioan\\WebstormProjects\\ecom-shop-react-ts-21\\src\\containers\\AllProductsPage\\index.tsx",["231","232"],"C:\\Users\\Ioan\\WebstormProjects\\ecom-shop-react-ts-21\\src\\utils\\helper.ts",[]
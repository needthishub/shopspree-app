e = () => {\r\n        this.setState({\r\n            theme: this.state.theme === 'light' ? 'dark' : 'light',\r\n        })\r\n    }\r\n\r\n    render() {\r\n        const {theme, showThemeButton} = this.state;\r\n        const isLightTheme = theme === 'light';\r\n        this.body.style.backgroundColor = isLightTheme ? 'white' : 'black';\r\n        const iconClassName = isLightTheme ? 'fa-sun' : 'fa-moon';\r\n\r\n        const themeButton = showThemeButton ? ReactDOM.createPortal(\r\n            <i className={`fas ${iconClassName} theme-context-button ${theme}`} onClick={this.handleChangeTheme}/>,\r\n            this.el\r\n        ): null;\r\n\r\n        return (\r\n            <ThemeContext.Provider value={theme}>\r\n                <div className={`app-container ${theme}`}>\r\n                    {this.props.children}\r\n                </div>\r\n                {themeButton}\r\n            </ThemeContext.Provider>\r\n        )\r\n    }\r\n}\r\n\r\nexport default ThemeContextProvider;\r\n","C:\\Users\\Ioan\\WebstormProjects\\ecom-shop-react-ts-21\\src\\components\\ProductCardModalQuantityUI\\index.tsx",[],"C:\\Users\\Ioan\\WebstormProjects\\ecom-shop-react-ts-21\\src\\ui-components\\Button\\index.tsx",[],"C:\\Users\\Ioan\\WebstormProjects\\ecom-shop-react-ts-21\\src\\components\\ProductCard\\index.tsx",[],"C:\\Users\\Ioan\\WebstormProjects\\ecom-shop-react-ts-21\\src\\components\\Pagination\\index.tsx",[],"C:\\Users\\Ioan\\WebstormProjects\\ecom-shop-react-ts-21\\src\\ui-components\\Checkbox\\index.tsx",[],{"ruleId":"238","replacedBy":"239"},{"ruleId":"240","replacedBy":"241"},{"ruleId":"238","replacedBy":"242"},{"ruleId":"240","replacedBy":"243"},{"ruleId":"238","replacedBy":"244"},{"ruleId":"240","replacedBy":"245"},{"ruleId":"238","replacedBy":"246"},{"ruleId":"240","replacedBy":"247"},{"ruleId":"238","replacedBy":"248"},{"ruleId":"240","replacedBy":"249"},{"ruleId":"238","replacedBy":"250"},{"ruleId":"240","replacedBy":"251"},{"ruleId":"238","replacedBy":"252